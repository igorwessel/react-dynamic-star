{
  "name": "react-dynamic-star",
  "version": "0.0.2",
  "description": "Single component to generate dynamic star svg",
  "main": "./dist/rds.umd.js",
  "module": "./dist/rds.es.js",
  "types": "./dist/rds.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "lint": "eslint src lib --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/rds.es.js",
      "require": "./dist/rds.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorwessel/react-dynamic-star.git"
  },
  "keywords": [
    "react dynamic star",
    "react star rating"
  ],
  "author": "Igor Wessel",
  "bugs": {
    "url": "https://github.com/igorwessel/react-dynamic-star/issues"
  },
  "homepage": "https://github.com/igorwessel/react-dynamic-star",
  "peerDependencies": {
    "react": ">=17.x"
  },
  "resolutions": {
    "@types/react": "17.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@swc/core": "^1.9.2",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "@vitejs/plugin-react": "1.0.0",
    "jest": "27.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.4.4",
    "vite": "2.6.4",
    "vite-plugin-dts": "0.9.2",
    "vite-tsconfig-paths": "3.3.17"
  }
}
